{
  "name": "lista-relacionamentos",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "prebuild": "node scripts/build-data.mjs",
    "build": "next build",
    "postbuild": "node scripts/generate-sitemap-gz.mjs && node scripts/check-core-web-vitals.mjs",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "check": "pnpm lint && pnpm typecheck && pnpm build",
    "import:sites": "node --env-file=.env.local scripts/import-sites-from-xlsx.mjs",
    "import:posts": "node --env-file=.env.local scripts/import-posts-from-xlsx.mjs",
    "images:init": "node --env-file=.env.local scripts/generate-initial-images.mjs",
    "images:upload": "node --env-file=.env.local scripts/upload-images-to-supabase.mjs",
    "sync:categories": "node --env-file=.env.local scripts/sync-categories-from-xlsx.mjs",
    "seed:ratings": "node --env-file=.env.local scripts/seed-ratings-from-xlsx.mjs",
    "import:comments": "node --env-file=.env.local scripts/import-comments-from-xlsx.mjs",
    "comments:randomize": "node --env-file=.env.local scripts/randomize-comment-dates.mjs",
    "comments:wipe": "node --env-file=.env.local scripts/wipe-comments.mjs",
    "comments:weighted": "node --env-file=.env.local scripts/import-comments-weighted.mjs",
    "comments:cleanup": "node --env-file=.env.local scripts/cleanup-comments.mjs",
    "lighthouse": "lighthouse http://localhost:3000 --output=json --output-path=./lighthouse-report.json --chrome-flags='--headless'",
    "core-web-vitals": "node scripts/check-core-web-vitals.mjs",
    "check-css": "node scripts/check-css-optimization.mjs"
